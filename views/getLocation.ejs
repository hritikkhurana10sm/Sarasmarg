  <div id="map"></div>

  <script>

var map = L.map('map').setView([12.95322, 77.58004], 15);
L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {
    maxZoom: 25,
    attribution: 'Â© OpenStreetMap'
}).addTo(map);
var latLon = L.latLng(12.95322, 77.58004);
var bounds = latLon.toBounds(500); // 500 = metres
map.panTo(latLon).fitBounds(bounds);


const arr = [<%-complaint%>]
for(var i = 0 ; i < arr.length ; i+=2){
console.log([arr[i],arr[i+1]]);
L.marker([arr[i],arr[i+1]]).addTo(map).bindPopup(`${arr[i]},${arr[i+1]}`).openPopup();
}



  </script>
